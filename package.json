{
  "name": "carrier-tracker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "npm run migrate:build && next build",
    "start": "next start",
    "lint": "next lint",
    "migrate:build": "node scripts/vercel-migrate.js",
    "migrate:manual": "curl -X GET \"$VERCEL_URL/api/migrate\" -H \"Authorization: Bearer $SUPABASE_SERVICE_ROLE_KEY\"",
    "db:start": "npx supabase start",
    "db:stop": "npx supabase stop",
    "db:reset": "npx supabase db reset",
    "db:migrate": "npx supabase db push",
    "db:seed": "npx supabase db seed",
    "db:setup": "node scripts/supabase-migrate.js"
  },
  "dependencies": {
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.50.2",
    "dotenv": "^17.0.0",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "next": "15.3.4",
    "pg": "^8.16.3",
    "react": "^19.0.0",
    "react-aria-components": "^1.10.1",
    "react-dom": "^19.0.0",
    "resend": "^4.6.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "supabase": "^2.26.9",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
